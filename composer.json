{
	"name": "zertex/yii2fullcalendar",
	"description": "Yii2 fullcalendar Widgets",
	"keywords": ["yii2", "fullcalendar","widget"],
	"homepage": "http://www.frenzel.net/",
	"type": "yii2-extension",
	"license": "MIT",
	"authors": [
		{
			"name": "Philipp Frenzel - Zertex Edition",
			"email": "philipp@frenzel.net",
			"homepage": "http://www.frenzel.net/",
			"role": "Developer"
		}
	],
	"support": {
		"source": "https://github.com/zertex/yii2fullcalendar"
	},
	"minimum-stability": "stable",
	"require": {
		"yiisoft/yii2": "*",
		"yiisoft/yii-jquery" : "*",
		"bower-asset/fullcalendar": "4.0.2",
		"bower-asset/fullcalendar-scheduler" : "4.0.1@stable"
	},
	"require-dev": {
		"codeception/codeception": "*"
	},
	"config": {
	    "process-timeout": 1800,
	      "fxp-asset": {
		"repositories": {
		  "bower-asset/eve": {
		  	"type": "bower-vcs",
		  	"url": "https://github.com/adobe-webplatform/eve.git"
		  }
		}
	    }
	},
	"conflict": {
		"bower-asset/fullcalendar": "^4.3.0"
	},
	"autoload": {
		"psr-4": { "yii2fullcalendar\\": "" }
	}
}
